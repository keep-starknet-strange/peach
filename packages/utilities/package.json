{
  "name": "utilities",
  "version": "0.0.0",
  "dependencies": {
    "react": "18.2.0",
    "react-native-device-info": "13.1.0",
    "react-test-renderer": "18.3.1"
  },
  "devDependencies": {
    "@peach/eslint-config": "workspace:^",
    "@testing-library/react-hooks": "8.0.1",
    "@types/react": "18.3.11",
    "@types/react-test-renderer": "18.3.0",
    "eslint": "8.44.0",
    "jest": "29.7.0",
    "jest-chrome": "0.8.0",
    "typescript": "5.3.3"
  },
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "build": "tsc -b",
    "check:deps:usage": "depcheck && yarn check-promise-version",
    "check-promise-version": "../../scripts/check-promise-version.sh",
    "format": "../../scripts/prettier.sh",
    "lint": "eslint . --ext ts,tsx --max-warnings=0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "jest",
    "typecheck": "tsc -b"
  },
  "sideEffects": [
    "*.css"
  ]
}
